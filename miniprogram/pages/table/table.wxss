/* pages/table/table.wxss */
@import "../lib/weui.wxss";
.picker1{
    padding: 12px;
}

.table {
    border: 0px solid darkgray;
   }
   .tr {
    display: flex;
    width: 100%;
    justify-content: center;
    height: 3rem;
    align-items: center;
    font-size: 12px;
   }
   .td {
    width:60%;
    justify-content: center;
    text-align: center;
   }
   .bg-w{
    background: snow;
   }
   .bg-g{
    background: #E6F3F9;
    font-size: 12px;
   }
   .th {
    width: 40%;
    justify-content: center;
    background: #6666FF;
    color: #fff;
    display: flex;
    height: 3rem;
    align-items: center;
   }